		$.ajax({
			dataType: "json",
			method: "GET",
			url: 'https://egol.fcf.com.br/egolligaSC/WsEgol.wso/Get_JogadoresClube/JSON/debug?sClube='+ clube.sCodClube +'&iCampeonato='+aCampeonato[0].iCampeonato+'&sLiga='+aCampeonato[0].sLiga,
