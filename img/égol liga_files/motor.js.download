function getJsonCampeonato(){
	
	$.ajax({
		dataType: "json",
		method: "GET",
		url: 'https://egol.fcf.com.br/egolligaSC/WsEgol.wso/Get_IdCampeonato/JSON/debug?sCampeonato='+campeonato_url,
