function criar_jogos(){
	temp_seq_rodada=0
	for(rodada of aRodadas){
		rodada.seq = temp_seq_rodada
		let idaVolta ="&nbsp;"
		let titulo_rodada
		titulo_rodada= rodada.sRodada
			
		$('.principal-jogos-container').append(
